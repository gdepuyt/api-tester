{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "FormattedCityName": {
        "type": "string"
      },
      "CityName": {
        "type": "string"
      },
      "Postalcode": {
        "type": "string",
        "pattern": "^[0-9]{4}$"
      }
    },
    "required": ["FormattedCityName", "CityName", "Postalcode"]
  }
}